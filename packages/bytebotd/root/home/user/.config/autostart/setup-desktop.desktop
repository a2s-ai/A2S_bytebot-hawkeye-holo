[Desktop Entry]
Type=Application
Name=Setup Desktop Icons
Comment=Ensure desktop icons are properly configured
Exec=/bin/bash -c "for f in /home/user/Desktop/*.desktop; do [ -f \"$f\" ] && chmod +x \"$f\" && gio set \"$f\" metadata::trusted true 2>/dev/null || true; done"
Terminal=false
StartupNotify=false
Hidden=false
NoDisplay=true
X-GNOME-Autostart-enabled=true
